<%- include ("../partials/navbar.ejs") %>

<% if (notes && notes.length > 0) { %>
  <% notes.forEach(function (noteItem) { %>
    <li class="notebook-holder">
      <div class="notebook">
        <form action="/note-book" method="GET">
          <button type="submit"><%= noteItem.id %></button>
        </form>
      </div>
    </li>
  <% }) %>
<% } %>